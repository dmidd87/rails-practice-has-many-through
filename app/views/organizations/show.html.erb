<div class="page-header">
  <h1>Showing Organization <%= @organization.name %></h1>
</div>

<h2>People</h2>

<table>
  <thead>
    <tr>
      <th>
        Name
      </th>
    </tr>
  </thead>
  <tbody>
    <div class="well">
      <ul>
      <% @organization.people.each do |people| %>
        <li><%= people.name %></li>
      <% end %>
    </ul>
    </div>
  </tbody>
</table>
