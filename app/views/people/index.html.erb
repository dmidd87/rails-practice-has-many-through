<div class="page-header">
  <h1>People</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>
        Name
      </th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td>
          <%= link_to person.name, person_path(person) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
